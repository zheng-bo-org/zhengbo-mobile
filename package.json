{
  "name": "zhengbo",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "npm run compileLspApi && npm run type-check && expo start",
    "android": "npm run type-check && expo run:android",
    "ios": "npm run type-check && expo run:ios",
    "web": "npm run type-check && expo start --web",
    "jest": "npm run type-check && jest",
    "type-check": "tsc",
    "compileLspApi": "node lspCompiler.js"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./__mocks__/setup.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "dependencies": {
    "expo": "~49.0.8",
    "expo-constants": "~14.4.2",
    "expo-linking": "~5.0.2",
    "expo-localization": "~14.3.0",
    "expo-router": "2.0.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "i18n-js": "^4.3.0",
    "jest": "^29.2.1",
    "jest-expo": "^49.0.0",
    "lsp-api": "^2.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.5",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "~0.19.6",
    "@react-native-async-storage/async-storage": "1.18.2",
    "react-native-svg": "13.9.0",
    "expo-image": "~1.3.4",
    "expo-dev-client": "~2.4.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.5",
    "@types/react": "~18.2.14",
    "typescript": "^5.1.3"
  },
  "private": true
}
